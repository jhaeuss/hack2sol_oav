{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "wielandOAV",
		"type": "application",
		"i18n": "i18n/i18n_de.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"mainService": {
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				},
				"uri": "localService/metadata.xml"
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.wieland.oav.view.Home",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.wieland.oav.i18n.i18n_de"
				}
			},
			"": {
				"dataSource": "mainService"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.wieland.oav.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl"
			},
			"targets": {
				"Home": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Home",
					"viewLevel": 1
				},
				"UserFreigabe": {
					"viewType": "XML",
					"viewName": "UserFreigabe",
					"viewLevel": 2,
					"clearControlAggregation": false,
					"transition": "slide"
				},
				"AvisierungAnmelden": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "OAV_Add",
					"viewLevel": 2
				}
			},
			"routes": [
				{
					"name": "UserFreigabe",
					"pattern": "UserFreigabe",
					"target": [
						"UserFreigabe"
					]
				},
				{
					"name": "Home",
					"pattern": "Home",
					"target": [
						"Home"
					]
				},
				{
					"name": "AvisierungAnmelden",
					"pattern": "AvisierungAnmelden",
					"target": [
						"AvisierungAnmelden"
					]
				}
			]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}