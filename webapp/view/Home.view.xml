<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.wieland.oav.controller.Home">
	<App id="idAppControl">
		<pages>
			<Page title="Online-Avisierung">
				<content>
					<HBox justifyContent="Center" width="100%">
						<items>
							<GenericTile xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Neuregistrierungen" subheader="Freigabe neuer User..." press="action" action:wiring="\{'press':\{'navigation':\{'routeName':'UserFreigabe'\}\}\}" id="NeuregistrierungenTile" visible="false">
								<tileContent>
									<TileContent>
										<content>
											<sap.ui.core:Icon xmlns:sap.ui.core="sap.ui.core" src="sap-icon://account" size="2em"/>
										</content>
									</TileContent>
								</tileContent>
							</GenericTile>
							<GenericTile xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Neue Avisierung" subheader="Avisierung anmelden..." press="action" action:wiring="\{'press':\{'navigation':\{'routeName':'AvisierungAnmelden'\}\}\}" id="AvisierungAnmeldenTile" visible="false">
								<tileContent>
									<TileContent>
										<content>
											<sap.ui.core:Icon xmlns:sap.ui.core="sap.ui.core" src="sap-icon://paper-plane" size="2em"/>
										</content>
									</TileContent>
								</tileContent>
							</GenericTile>
						</items>
					</HBox>
				</content>
			<headerContent>
			    <Image src="/webapp/img/wieland.jpg" id="image2"/>
			</headerContent>
			</Page>
		</pages>
	</App>
</mvc:View>